  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "npm run start:dev",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "sam:test": "rm -rf .aws-sam && npm run build && sam build && sam local start-api --port 8080 --env-vars ./configs/env.json",
    "deploy": "npm run build && sam build && sam validate && sam deploy",
    "deploy:prod": "npm run deploy --config-env prod",
    "sam:sqs": "npm run build && sam build && sam local invoke --env-vars ./configs/env.json --event ./events/event-sqs.json ConsumerFunction",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
