{
  "moduleFileExtensions": ["js", "json", "ts"],
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "rootDir": "../",
  "moduleDirectories": ["<rootDir>/../", "node_modules"],
  "moduleNameMapper": {
    "@/(.*)": "<rootDir>/src/$1"
  }
}
